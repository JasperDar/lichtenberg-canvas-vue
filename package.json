{
  "name": "lichtenberg-canvas-vue",
  "private": false,
  "version": "0.0.4",
  "author": "Jasper Dar",
  "license": "MIT",
  "description": "A Vue.js component to render Lichtenberg figure-like effects in the background.",
  "type": "module",
  "files": [
    "dist",
    "lib"
  ],
  "main": "./dist/lichtenberg-canvas.umd.cjs",
  "module": "./dist/lichtenberg-canvas.js",
  "exports": {
    ".": {
      "import": "./dist/lichtenberg-canvas.js",
      "require": "./dist/lichtenberg-canvas.umd.cjs"
    },
    "./style.css": "./dist/lichtenberg-canvas.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/node": "^22.10.5",
    "@vueuse/core": "^12.4.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vue-tsc": "^2.2.0"
  }
}
